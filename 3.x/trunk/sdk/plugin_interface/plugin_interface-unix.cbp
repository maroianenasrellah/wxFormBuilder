<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="5" />
	<Project>
		<Option title="Plugin Interface (Unix)" />
		<Option pch_mode="2" />
		<Option default_target="" />
		<Option compiler="gcc" />
		<Option virtualFolders="" />
		<Build>
			<Target title="Debug">
				<Option output="lib\libplugin_interfaced.a" prefix_auto="0" extension_auto="0" />
				<Option working_dir="" />
				<Option object_output=".objd\" />
				<Option type="2" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Compiler>
					<Add option="-g" />
					<Add option="-D_DEBUG" />
					<Add option="-D__WXDEBUG__" />
				</Compiler>
				<Environment>
					<Variable name="DEBUG" value="yes" />
				</Environment>
			</Target>
			<Target title="Release">
				<Option output="lib\libplugin_interface.a" prefix_auto="0" extension_auto="0" />
				<Option working_dir="" />
				<Option object_output=".obj\" />
				<Option type="2" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-DNDEBUG" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
				<Environment>
					<Variable name="DEBUG" value="no" />
				</Environment>
			</Target>
			<Environment>
				<Variable name="tinyxml" value="..\tinyxml" />
			</Environment>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="`wx-config --debug=$(DEBUG) --cflags`" />
			<Add option="-D__WX__" />
			<Add option="-DWXUSINGDLL" />
			<Add option="-DTIXML_USE_TICPP" />
			<Add directory="$(tinyxml)" />
		</Compiler>
		<Linker>
			<Add directory="..\lib" />
		</Linker>
		<Unit filename="component.h">
			<Option compilerVar="CPP" />
			<Option compile="0" />
			<Option link="0" />
			<Option target="Debug" />
			<Option target="Release" />
		</Unit>
		<Unit filename="plugin.h">
			<Option compilerVar="CPP" />
			<Option compile="0" />
			<Option link="0" />
			<Option target="Debug" />
			<Option target="Release" />
		</Unit>
		<Unit filename="xrcconv.cpp">
			<Option compilerVar="CPP" />
			<Option target="Debug" />
			<Option target="Release" />
		</Unit>
		<Unit filename="xrcconv.h">
			<Option compilerVar="CPP" />
			<Option compile="0" />
			<Option link="0" />
			<Option target="Debug" />
			<Option target="Release" />
		</Unit>
		<Extensions />
	</Project>
</CodeBlocks_project_file>