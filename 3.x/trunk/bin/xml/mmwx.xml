<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<package name="mmwx" lib="libmmwx" desc="">
  <objectinfo class="mmMultiButton" icon="button.xpm" type="widget">
    <inherits class="wxWindow" />
    <property name="name" type="text">m_mmBtn</property>
    <property name="default_bp" type="bitmap" />
    <property name="focus_bp" type="bitmap" />
    <property name="sel_bp" type="bitmap" />
    <property name="toggle_bp" type="bitmap" />
    <property name="style" type="bitlist">
      <option name="mmMB_FOCUS" />
      <option name="mmMB_SELECT" />
      <option name="mmMB_TOGGLE" />
      <option name="mmMB_DROPDOWN" />
      <option name="mmMB_WHOLEDROPDOWN" />
      <option name="mmMB_STATIC" />
      <option name="mmMB_NO_AUTOSIZE" />
      <option name="mmMB_AUTODRAW" />0</property>
    <property name="label" type="wxString">mmMultiButton</property>
    <codegen language="C++">
      <template name="declaration">mmMultiButton *$name;</template>
      <template name="construction">$name = new mmMultiButton(#wxparent $name, $id, $label, $default_bp, $pos, $size, $style);</template>
      <template name="include">#include &lt;wx/mmwx/mmMultiButton.h&gt;</template>
    </codegen>
  </objectinfo>
  <objectinfo class="mmMonthCtrl" icon="button.xpm" type="widget">
    <inherits class="wxWindow" />
    <property name="name" type="text">m_mmMonthCtrl</property>
    <property name="style" type="bitlist">
      <option name="mmSHOW_TODAY" />
      <option name="mmSHOW_SELECT" />
      <option name="mmSHOW_MOVE" />
      <option name="mmSHOW_BORDER_DAYS" />
      <option name="mmSHOW_ALL" />
      <option name="mmPREV_BTN" />
      <option name="mmNEXT_BTN" />
      <option name="mmRESCALE_FONTS" />0</property>
    <codegen language="C++">
      <template name="declaration">mmMonthCtrl *$name;</template>
      <template name="construction">$name = new mmMonthCtrl(#wxparent $name, $id, wxDateTime::Today(), $pos, $size, $style);</template>
      <template name="include">#include &lt;wx/mmwx/mmMonthCtrl.h&gt;</template>
    </codegen>
  </objectinfo>
</package>
