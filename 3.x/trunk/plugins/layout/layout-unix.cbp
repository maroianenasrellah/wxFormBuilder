<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="5" />
	<Project>
		<Option title="Layout Components Plugin (Unix)" />
		<Option pch_mode="0" />
		<Option default_target="" />
		<Option compiler="gcc" />
		<Option virtualFolders="" />
		<Build>
			<Target title="Debug">
				<Option output="../../bin/plugins/layout/liblayoutd.so" prefix_auto="0" extension_auto="0" />
				<Option object_output=".objsd" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
					<Add option="-D_DEBUG" />
					<Add option="-D__WXDEBUG__" />
				</Compiler>
				<Environment>
					<Variable name="SUFFIX" value="d" />
					<Variable name="DEBUG" value="yes" />
				</Environment>
			</Target>
			<Target title="Release">
				<Option output="../../bin/plugins/layout/liblayout.so" prefix_auto="0" extension_auto="0" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Environment>
					<Variable name="DEBUG" value="no" />
				</Environment>
			</Target>
			<Environment>
				<Variable name="sdk" value="../../sdk" />
				<Variable name="SUFFIX" value="" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="`wx-config --debug=$(DEBUG) --cflags`" />
			<Add option="-DBUILD_DLL" />
			<Add option="-DWXUSINGDLL" />
			<Add option="-DNOPCH" />
			<Add option="-DNO_GCC_PRAGMA" />
			<Add option="-DTIXML_USE_TICPP" />
			<Add option="-D_UNICODE" />
			<Add directory="$(sdk)/tinyxml" />
			<Add directory="$(sdk)/plugin_interface" />
		</Compiler>
		<Linker>
			<Add option="`wx-config --debug=$(DEBUG) --libs`" />
			<Add option="-Wl,-rpath,$``ORIGIN../lib" />
			<Add library="fbPluginInterface$(SUFFIX)" />
			<Add library="ticpp$(SUFFIX)" />
			<Add directory="$(sdk)/lib" />
		</Linker>
		<Unit filename="layout.cpp">
			<Option compilerVar="CPP" />
			<Option target="Debug" />
			<Option target="Release" />
		</Unit>
		<Extensions />
	</Project>
</CodeBlocks_project_file>